{
  "name": "sql-to-ts",
  "version": "1.0.10",
  "description": "Generate Typescript from SQL",
  "keywords": [],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "bin": {
    "sql-to-ts": "./index.js"
  },
  "scripts": {
    "test": "npm run rawtest",
    "clean": "del-cli dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "prebuild": "npm run clean",
    "build": "tsc",
    "pretest": "npm run format && npm run build",
    "rawtest": "ts-node test/index.ts",
    "prepublishOnly": "npm run rawtest && npm run build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/cli": "^0.11.19",
    "del-cli": "^1.1.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/node": "^8.10.44",
    "cli": "^1.0.1",
    "del": "^4.0.0",
    "mkdirp": "^0.5.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/sql-to-ts.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/sql-to-ts/issues"
  },
  "homepage": "https://github.com/beenotung/sql-to-ts#readme"
}
